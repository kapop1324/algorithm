import java.util.*;

public class Main {	
    public static void main(String[] args)  {
    	
    	String array[] = new String[4];
    	
    
    	Scanner sc = new Scanner(System.in);
    	
    		for(int i =0; i<4; i++) {
    			array[i] = sc.next();
    		}
    		String num1 = array[0]+array[1];
    		String num2 = array[2]+array[3];
    		
    		long answer = Long.valueOf(num1)+Long.valueOf(num2);
    		
    		System.out.println(answer);
    	
    	   	
   	}    	   	        
}
