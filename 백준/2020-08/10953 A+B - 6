import java.util.*;

public class Main {	
    public static void main(String[] args) {
    	
    	Scanner sc = new Scanner(System.in);
    	int n = sc.nextInt();
    	
    	for(int i =0; i<n; i++) {
    		
    		String[] s = sc.next().split(",");
    		int a = Integer.parseInt(s[0]);
        	int b = Integer.parseInt(s[1]);
        	
        	System.out.println(a+b);
    	}
    	
    	
        
    }
    
  
}
