import java.util.Scanner;
public class Main {

	public static void main(String[] args)   {
	
		 Scanner sc =new Scanner(System.in);
		 int n = sc.nextInt();
		 int count=0;
		 int i = 0;
		 
		while(true) {
			i++;
			if(n <= i*(i-1)*3+1) {
				System.out.println(i);
				break;
			}
			
		}
		 
		 
		 
		 
		 
		
	}
		
		
}

	
