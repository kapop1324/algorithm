import java.util.Scanner;
public class Main {

	public static void main(String[] args)   {
		 long a,b,v;
		 Scanner sc =new Scanner(System.in);
		 a = sc.nextInt();
		 b = sc.nextInt();
		 v = sc.nextInt();
		 
		 long h = 0;
		
		
		 
		h = (v-b)/(a-b);
		if((v-b)%(a-b) != 0) {
			h++;
		}
		
		 
		 System.out.println(h);
		
	}
		
}

	
