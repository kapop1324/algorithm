import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	
		Scanner sc = new Scanner(System.in);
		int n;
		int a,b;
		int min=2000;
		
		
		int sum = 0;
		for(int i=0; i<3; i++) {
			n=sc.nextInt();
			
			if(min>n) {
				min =n ;
			}
			
		}
		a = min;
		for(int i=0; i<2; i++) {
			n=sc.nextInt();
			
			if(min>n) {
				min =n ;
			}
		}
		b = min;
		System.out.println(a+b-50);
		
		
	}
	

}
