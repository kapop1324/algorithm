import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int n,min = 1000000,max = -1000000;
		n = sc.nextInt();
		
		
		int array[] = new int[n];
		
		
		for(int i =0; i<n ; i++) {
			array[i] = sc.nextInt();
			
			
				if(array[i] < min) {
					min = array[i];
				}
				
				if(array[i] > max) {
					max = array[i];
				}
			}
			
			System.out.println(min+" "+max);
			
		}
		
		
		
		

	}
	
