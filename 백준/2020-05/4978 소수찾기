import java.util.Scanner;
public class Main {

	public static void main(String[] args)   {
		
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int a;
		
		int result = 0;
		for(int i =0; i<n; i++) {
			int count = 0;
			a = sc.nextInt();
			for(int j=2; j<=a; j++) {
				if(a%j ==0) {
					count++;
				}
			}
			if(count==1) {
				result++;
			}
		}
		
		sc.close();
		System.out.println(result);
		
		
		
		

	}
	


	
	
	
		
		
	
		
}

	

