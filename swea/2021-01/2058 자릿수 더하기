
class Solution
{
    public static void main(String args[]) throws Exception
    {
        Scanner sc = new Scanner(System.in);
         
        String a = sc.next();
        int sum = 0;
         
        for(int i = 0; i < a.length(); i++) {
             
            sum += Integer.parseInt(a.substring(i,i+1));
             
        }
         
        System.out.println(sum);
         
         
    }
}
