
import java.util.Scanner;
import java.io.FileInputStream;
 

class Solution
{
    public static void main(String args[]) throws Exception
    {
            Scanner sc = new Scanner(System.in);
         
        String a = sc.next();
         
        for(int i = 0; i < a.length(); i++) {
             
                 
         
                System.out.print(a.charAt(i)-64+" ");
             
             
             
         
        }
         
         
    }
}
